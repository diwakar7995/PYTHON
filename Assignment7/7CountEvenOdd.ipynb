    "Even,Odd=0,0\n",
    "Num=int(input(\"Enter The nth Number=\"))\n",
    "for i in range(1,Num+1):\n",
    "    if(i%2==0):\n",
    "        Even=Even+1\n",
    "    else:\n",
    "        Odd=Odd+1\n",
    "print(\"Number Even Numbers In The Series Are\",Even)\n",
    "print(\"Number Odd Numbers In The Series Are\",Odd)"
