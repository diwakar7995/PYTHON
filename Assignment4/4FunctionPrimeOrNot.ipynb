   "source": [
    "def prime_Num(Num):\n",
    "    for i in range(2,Num//2):\n",
    "        if(Num%i!=0):\n",
    "            return False\n",
    "    else:\n",
    "        return True\n",
    "Num=int(input(\"Enter The Number=\"))\n",
    "if prime_Num(Num):\n",
    "    print(Num,\"Is A Prime Number\")\n",
    "else:\n",
    "    print(Num,\"Is Not A Prime Number\")"
   ]
